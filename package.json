{
  "name": "exograph-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "test": "mocha --timeout 15000 --ui bdd --recursive ./test/unit",
    "build-css": "node-sass public/scss/test.scss public/css/test.css",
    "startdev": "nodemon bin/www",
    "assets": "node-sass ~/Exograph-Server/public/sass -o ~/Exograph-Server/public/css",
    "start-selenium": "java -jar -Dwebdriver.gecko.driver=./test/selenium/drivers/geckodriver -Dwebdriver.chrome.driver=./test/selenium/drivers/chromedriver test/selenium/drivers/selenium-server-standalone-3.1.0.jar",
    "run-wdio": "node_modules/.bin/wdio wdio.conf.js "
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "~1.15.2",
    "bootstrap-sass": "^3.3.7",
    "connect-flash": "latest",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "~4.14.0",
    "express-fileupload": "^0.1.2",
    "express-session": "~1.0.0",
    "jade": "~1.11.0",
    "morgan": "~1.7.0",
    "neo4j": "^2.0.0-RC2",
    "nodemon": "^1.11.0",
    "npm-start": "^1.4.2",
    "passport": "~0.1.17",
    "passport-local": "~0.1.6",
    "request": "^2.79.0",
    "serve-favicon": "~2.3.0",
    "sha256": "^0.2.0"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "chai-http": "^3.0.0",
    "mocha": "^3.2.0",
    "nodemon": "^1.11.0",
    "proxyquire": "^1.7.11",
    "rewire": "^2.5.2",
    "sinon": "~1.17.7",
    "wdio-mocha-framework": "^0.5.10",
    "webdriverio": "^4.8.0",
    "chai-spies": "latest"
  }
